<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="customersListBrush">#222</SolidColorBrush>
        </ResourceDictionary>
        
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="customersListBrush">#EEE</SolidColorBrush>
        </ResourceDictionary>
        
    </ResourceDictionary.ThemeDictionaries>
    
    <SolidColorBrush x:Key="headerThemeBrush">#F05A28</SolidColorBrush>

</ResourceDictionary>
